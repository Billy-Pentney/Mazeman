<Window x:Class="A_Level_Project__New_.LeaderboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:A_Level_Project__New_"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Leaderboard" Height="400" Width="400">
    <Canvas Name="LeaderboardCanvas">
        <Label Name="PositionLbl" Content="Position" Canvas.Left="10" Canvas.Top="10" HorizontalContentAlignment="Center"></Label>
        <Label Name="NameLbl" Content="Name" Canvas.Left="90" Canvas.Top="10" HorizontalContentAlignment="Center" Width="107"></Label>
        <Label Name="ScoreLbl" Content="Score" Canvas.Left="222" Canvas.Top="10" HorizontalContentAlignment="Center" Width="50"></Label>
        <TextBlock Name="PositionTxtBlock" Width="52" Height="318" Canvas.Left="10" Canvas.Top="41" TextAlignment="Center"></TextBlock>
        <TextBlock Name="NameTxtBlock" Width="107" Height="318" Canvas.Left="90" Canvas.Top="41" TextAlignment="Center"></TextBlock>
        <TextBlock Name="ScoreTxtBlock" Width="50" Height="318" Canvas.Left="222" Canvas.Top="41" TextAlignment="Center"></TextBlock>
    </Canvas>
</Window>
